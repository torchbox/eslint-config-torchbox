    "eslint-plugin-react": "^7.14.2",
    "prettier": "^1.18.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-react": "^7.14.2"
  },
  "scripts": {
    "lint": "eslint --report-unused-disable-directives . && prettier --check '**/?(.)*.{md,css,scss,js,json,yaml,yml,html}'",
